{
  "name": "frontend-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npx ng serve",
    "start:proxy": "npx ng serve --proxy-config proxy.conf.json",
    "build": "npx ng build",
    "build:prod": "npx ng build --configuration production",
    "watch": "npx ng build --watch --configuration development",
    "test": "npx ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.0",
    "typescript": "~5.9.2"
  }
}
